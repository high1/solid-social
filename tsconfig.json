{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["DOM", "ESNext"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "types": [],
    "declaration": true,
    "declarationDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  }
}
